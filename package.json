{
  "name": "cc-cookiecomply",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force .next, out, dist -ErrorAction SilentlyContinue\"",
    "clean-cache": "powershell -Command \"Remove-Item -Recurse -Force .next, out, dist, .cache, cache, webpack -ErrorAction SilentlyContinue\"",
    "type-check": "tsc --noEmit",
    "deploy": "npm run clean-cache && npm run build && npx wrangler pages deploy .next/standalone --project-name calculator",
    "stagewise": "stagewise",
    "stagewise:analyze": "stagewise analyze",
    "stagewise:generate": "stagewise generate",
    "stagewise:refactor": "stagewise refactor"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "next": "^15.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "stagewise": "^0.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}
